    "require": {
        "php": ">=5.4.0",
        "cocur/slugify": "^2.3",
        "illuminate/support": "5.*.*"
    },
    "authors": [
        {
            "name": "Sam Taylor",
            "email": "sam@taylornetwork.ca"
        }
    ],
    "autoload": {
    	"psr-4": {
    		"TaylorNetwork\\ModelSlugger\\": "src/"
    	}
    },
    "extra": {
        "laravel": {
